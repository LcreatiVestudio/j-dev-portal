{
  "name": "j-dev-portal",
  "version": "1.0.0-SNAPSHOT",
  "description": "A frontend source code for J-Dev Portal website",
  "private": true,
  "author": {
    "name": "Riichi Rusdiana",
    "email": "finnsonalca123@gmail.com",
    "url": "https://j-dev.xyz"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "rebuild": "rmdir dist /s /q && npm run build",
    "dev": "vue-cli-service build --mode development",
    "redev": "rmdir dist /s /q && npm run dev",
    "lint": "vue-cli-service lint",
    "git:precommit": "git add . && git commit -am \"Precommit made from NPM\" && git push",
    "git:deploy": "git add . && git commit -am \"Codeship Deploy from NPM\" && git push"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "page": "^1.11.4",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "lint-staged": "^8.1.4",
    "standard": "^12.0.1",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21",
    "surge": "^0.20.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
